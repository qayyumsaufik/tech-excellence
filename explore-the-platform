<!--prevent whitespace in inputs-->
<script>
$(document).on('blur', 'input[type="text"], input[type="email"], textarea', function () {
  const val = $(this).val();

  if (val && /^\s+/.test(val)) {
    $(this).val(val.replace(/^\s+/, ''));
  }
});
</script><script id="chilipiper-concierge" src="https://fringe.chilipiper.com/concierge-js/cjs/concierge.js" crossorigin="anonymous"></script>

<script>
  (function () {
    if (window.__cpInit) return; 
    window.__cpInit = true;

    const FORM_SEL = '#wf-form-Book-a-demo';
    const form = document.querySelector(FORM_SEL);
    if (!form) return;

    // --- PREFILL from localStorage (same behavior as your separate script)
    const storedEmail = localStorage.getItem('formEmail');
    const emailEl = form.querySelector('input[plg-field="Email"], input[type="email"]');
    if (emailEl && storedEmail) emailEl.value = storedEmail;

    // --- Persist email to localStorage as user types + on submit
    const saveEmail = () => {
      if (!emailEl) return;
      localStorage.setItem('formEmail', (emailEl.value || '').trim());
    };
    if (emailEl) {
      ['input','change','blur'].forEach(evt => emailEl.addEventListener(evt, saveEmail));
    }

    // --- Email blocklist (unchanged logic)
    function isBlockedEmail(email) {
      const basicOK = /^[^\s@]+@[A-Za-z0-9.-]+\.[A-Za-z]{2,24}$/.test(email);
      const blocked = /(mailinator\.com|business\.com|workshop\.com|edu\.com|developer\.co|dev\.test|emailcompany\.com|compt\.io|joinforma\.com|bonusly\.com|joon\.io|dev\.te|getmail\.io|busines\.com|kennedy\.com|example\.com)$/i.test(email);
      return !basicOK || blocked;
    }

    // --- Form submit: validate, store your other fields, no re-dispatch
    form.addEventListener('submit', function (e) {
      const emailVal = (emailEl?.value || '').trim();

      if (isBlockedEmail(emailVal)) {
        e.preventDefault();
        alert('Email domain is not allowed.');
        saveEmail(); // still remember what they typed
        return;
      }

      // Build formData for your other fields
      const formData = {};
      form.querySelectorAll('[plg-field]:not([plg-field="programsExploring"])').forEach(el => {
        formData[el.getAttribute('plg-field')] = el.value;
      });

      const collectChecked = (attr) => {
        const arr = [];
        form.querySelectorAll(`[plg-field="${attr}"]`).forEach(block => {
          if (block.querySelector('.w-checkbox-input')?.classList.contains('w--redirected-checked')) {
            const name = block.querySelector('input[type="checkbox"]')?.name;
            if (name) arr.push(name);
          }
        });
        return arr;
      };

      formData.programsExploring = collectChecked('programsExploring');
      formData.problemToSolve   = collectChecked('problemToSolve');
      localStorage.setItem('formData', JSON.stringify(formData));

      saveEmail(); // persist latest value
      // NOTE: don't preventDefault here—let Chili Piper catch the native submit once.
    });

    // --- Success UI after Chili Piper finishes
    function showSuccessUI() {
      const wrap = form.closest('.w-form');
      const done = wrap?.querySelector('.w-form-done');
      const fail = wrap?.querySelector('.w-form-fail');
      if (fail) fail.style.display = 'none';
      form.style.display = 'none';
      if (done) done.style.display = 'block';
    }

    // --- SINGLE Chili Piper deploy (remove any other deploy blocks elsewhere)
    ChiliPiper.deploy('fringe', 'talk_to_our_team', {
      formType: 'HTML',
      formSelector: FORM_SEL,
      onRouted: showSuccessUI,
      onDisqualified: showSuccessUI,
      onError: function () {
        const fail = form.closest('.w-form')?.querySelector('.w-form-fail');
        if (fail) fail.style.display = 'block';
      }
    });
  })();
</script>

<script>
  $(document).ready(function () {
    $("form").on("submit", function () {
      const companySize = $(this).find("[name='companySize']").val();

      if (companySize && companySize !== "< 100 employees") {
        window.dataLayer = window.dataLayer || [];
        window.dataLayer.push({
          event: "generate_lead",
          company_size: companySize
        });
      }
    });
  });
</script></body></html>